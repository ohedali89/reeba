                        //calculation - items & update on ATC button
                        //prod_elements['no_of_items_label'] = current_qty == 1 ? current_qty + " ITEM" : current_qty + " ITEMS";
                        //$("#shopify-section-product-template button.add_to_cart .content_price").text("$"+prod_elements['new_price']);
                        //$("#shopify-section-product-template button.add_to_cart .item_count_data").text("(" + prod_elements['no_of_items_label'] + ")");
                    }
                }

                // your code
                var modal = document.getElementById("myModal");
                var btn = document.getElementById("myBtn");
                var span = document.getElementsByClassName("close")[0];
                //$('.table_section_add').html(get_table);
                btn.onclick = function () {
                    modal.style.display = "block";
                };
                span.onclick = function () {
                    modal.style.display = "none";
                };

                window.onclick = function (event) {
                    if (event.target == modal) {
                        modal.style.display = "none";
                    }
                };

                $('#productSizeChartVariation .table_section_add .modal_right .measurements_get span.select_sizes').on('click', function () {
                    var get_name = $(this).data('id');
                    $('#productSizeChartVariation .table_section_add .modal_left img').hide();
                    $('#productSizeChartVariation .table_section_add .modal_left img[data-id=' + get_name + ']').show();
                });

            })(jQuery);
        });
    </script>
