                });

                $(document).on('click', '.quantity_box .product_quant', function () {
                    $(this).hasClass("plus") ? manage_qty_calculation($(this), "plus") : manage_qty_calculation($(this), "minus");
                });

                // update Qty
                function manage_qty_calculation($this, operation) {
                    var prod_elements = [];
                    //get current qty
                    var get_qty = $('.quantity_box .quantity').val();

                    var current_qty;
                    if (operation == 'plus') {
                        current_qty = ++get_qty;
                    } else {
                        current_qty = --get_qty;
                    }

                    current_qty = parseInt(current_qty);
                    if (get_qty >= 1 && get_qty <= 10) {
                        $('.quantity_box .quantity').val(current_qty);
                        //get current price
                        //var current_price = $(".modal_price .current_price .money").text();
                        //current_price = parseFloat(current_price.substring(1, current_price.length)); //remove $ and convert into math format
                        //calculation - qty & update in qty box
                        //current_qty++;
                        //prod_elements['current_qty'] = current_qty;
